<PreferenceScreen

    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:settings="http://schemas.android.com/apk/res/com.android.settings"
    android:title="@string/rr_tweaks_title">

     <Preference
                android:key="magisk_settings"
                android:title="@string/magisk_title"
                android:icon="@drawable/ic_settings_magisk">
	            <intent
                    android:action="android.intent.action.MAIN" 
                    android:targetPackage="com.topjohnwu.magisk" 
                    android:targetClass="com.topjohnwu.magisk.SplashActivity"/>
     </Preference>
     
     <Preference
                android:key="themes_settings"
                android:title="@string/rr_themes_title"
                android:icon="@drawable/rr_themes">
                 <intent
                    android:action="android.intent.action.MAIN"
                    android:targetPackage="projekt.substratum"
                    android:targetClass="projekt.substratum.AppIntroActivity" />
     </Preference>
     
     <Preference
                android:key="performance_settings"
                android:title="@string/rr_performance_title"
                android:icon="@drawable/rr_performance_icon">
	            <intent
                    android:action="android.intent.action.MAIN" 
                    android:targetPackage="com.grarak.kerneladiutor" 
                    android:targetClass="com.grarak.kerneladiutor.activities.StartActivity"/>
     </Preference>
	    	
     <Preference
               android:key="adaway_settings"
               android:title="@string/rr_adaway_title"
               android:icon="@drawable/rr_adaway_icon">
                <intent
                    android:action="android.intent.action.MAIN"
                    android:targetPackage="org.adaway"
                    android:targetClass="org.adaway.ui.BaseActivity" />
     </Preference>
          
     <Preference
               android:key="viper4arise_settings"
               android:title="@string/rr_viper4arise_title"
               android:icon="@drawable/rr_animations_icon">
                <intent
                    android:action="android.intent.action.MAIN"
                    android:targetPackage="com.audlabs.viperfx"
                    android:targetClass="com.audlabs.viperfx.main.MainActivity" />
     </Preference>
	
</PreferenceScreen>
